@import "https://fonts.googleapis.com/css2?family=M+PLUS+Rounded+1c:wght@800&display=swap";*,*::before,*::after{box-sizing:border-box;text-decoration-thickness:.08em;text-decoration-skip-ink:none;border-radius:.5rem}section,ul,ol,dl,p,hr,figure,pre,blockquote,details,aside{margin:1.5rem 0}body{max-width:65em;margin:auto;padding:2rem 1rem;font-size:1rem;line-height:1.65;font-family:system-ui,-apple-system,BlinkMacSystemFont,segoe ui,Roboto,open sans,helvetica neue,sans-serif;overflow-x:hidden;overflow-y:scroll;color:#34495e;display:flex;flex-direction:column;min-height:100vh}header,footer{background:#ecf0f1;padding:2rem 1.5rem;padding-left:calc(50vw - 50%);padding-right:calc(50vw - 50%)}header{margin:-2rem calc(0px - (50vw - 50%))2rem}header h1{margin:0}header h1 a{text-decoration:none;color:#34495e}footer{margin:2rem calc(0px - (50vw - 50%))-2rem;margin-top:auto}aside{border:.2rem solid #bdc3c7;padding:1rem}h1,h2,h3,h4,h5,h6{line-height:1.2;margin:1.5em 0 1rem;font-family:"m plus rounded 1c",serif;font-weight:800;font-style:normal}h1{font-size:1.6rem}h2{font-size:1.4rem}ul,ol{padding-left:1rem;list-style-position:inside}dl{padding-left:1rem}dl dt{font-weight:700}dl dd{margin-left:1rem}a{color:#3498db}pre,code,kbd,samp,time{font-family:ui-monospace,Menlo,Monaco,segoe ui mono,roboto mono,consolas,monospace}pre,code{color:#27ae60}small,del,blockquote,figcaption,time,kbd{color:#7f8c8d}figcaption,aside,small{font-size:.8rem!important}blockquote{padding-left:2rem}summary{cursor:pointer}figure{line-height:1.2;margin:auto;text-align:center}figure figcaption{text-align:center}img{max-width:100%;max-height:90vh;width:auto;height:auto}hr{border:none;height:0;border-bottom:.2rem solid #bdc3c7}details{margin-left:2rem}details summary{margin-left:-1rem;color:#3498db}